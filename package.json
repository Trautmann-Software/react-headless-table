{
  "name": "@trautmann/react-headless-table-monorepo",
  "version": "0.0.2-beta.6",
  "description": "This is a monorepo; see src/package.json for more info",
  "private": true,
  "packageManager": "npm@9.2.0",
  "workspaces": [
    "docs",
    "package"
  ],
  "scripts": {
    "set-package-versions": "NODE_NO_WARNINGS=1 node docs/scripts/set-package-versions.mjs",
    "build": "turbo run build --no-daemon",
    "dev": "turbo run dev --parallel --no-daemon",
    "start": "npx serve docs/out",
    "lint": "turbo run lint --no-daemon",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "npm": ">=9.2.0",
    "node": ">=16.17.1"
  }
}
